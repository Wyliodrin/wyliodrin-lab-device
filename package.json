{
  "name": "wyliolab",
  "version": "1.1.12",
  "description": "wyliodrin lab software device controller",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Wyliodrin/wyliodrin-lab-device.git"
  },
  "keywords": [
    "iot",
    "raspberry",
    "pi",
    "wyliodrin"
  ],
  "author": "Wyliodrin",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/Wyliodrin/wyliodrin-lab-device/issues"
  },
  "bin": {
    "wyliolab": "build/index.js"
  },
  "homepage": "https://github.com/Wyliodrin/wyliodrin-lab-device#readme",
  "devDependencies": {
    "grunt": "^1.0.3",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-eslint": "^21.0.0"
  },
  "eslintConfig": {
    "env": {
      "es6": true,
      "node": true
    },
    "parserOptions": {
      "ecmaVersion": 2017,
      "sourceType": "script"
    },
    "plugins": [],
    "extends": [
      "eslint:recommended"
    ],
    "rules": {
      "indent": [
        "error",
        "tab",
        {
          "SwitchCase": 1
        }
      ],
      "linebreak-style": [
        "error",
        "unix"
      ],
      "no-console": [
        "warn"
      ],
      "quotes": [
        "error",
        "single"
      ],
      "semi": [
        "error",
        "always"
      ],
      "require-await": [
        "error"
      ]
    }
  },
  "dependencies": {
    "axios": "^0.18.0",
    "child-process-promise": "^2.2.1",
    "dotenv": "^6.0.0",
    "fs-extra": "^7.0.0",
    "ip": "^1.1.5",
    "lcd": "^2.0.3",
    "lodash": "^4.17.10",
    "msgpack5": "^4.2.1",
    "onoff": "^3.2.1",
    "pty.js": "^0.3.1",
    "ws": "^6.0.0"
  }
}
